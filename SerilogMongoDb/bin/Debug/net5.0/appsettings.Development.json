{
  "ConnectionStrings": {
    "LogMongoDbConnection": "mongodb://root:example@mongo:27017"
  },
  "TesteAmbiente": "Trinh Hung Nam here",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Logging",
        "Args": {
          "databaseUrl": "mongodb://root:example@mongo:27017",
          "collectionName": "Logging",
          "cappedMaxSizeMb": "1024",
          "cappedMaxDocuments": "50000"
        }
      }
    ]
  }
}
