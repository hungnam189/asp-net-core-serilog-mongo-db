{
  "ConnectionStrings": {
    "BaseLog": "mongodb://namth:namth2@localhost:27017/logging"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MongoDBBson",
        "Args": {
          "databaseUrl": "mongodb://namth:namth2@localhost:27017/logging",
          "collectionName": "logapi",
          "cappedMaxSizeMb": "1024",
          "cappedMaxDocuments": "50000"
        }
      }
    ]
  }

}